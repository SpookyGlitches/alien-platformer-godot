[application]

config/name="The Lost Alien"
config/description="Control the player by the arrow keys and spacebar as it continually runs forward. Along  the way, you have to collect key/s that will help you pass through a door to proceed to a higher level while avoiding various obstacles. The game has seven different levels, and as it  progresses, the intensity changes as the game advances along with the environment and  enemyâ€™s appearance. The goal of the game is to finish all levels with the shortest possible time."
run/main_scene="res://GUI/MainMenu.tscn"
config/use_custom_user_dir=true
boot_splash/bg_color=Color( 0, 0, 0, 1 )


[audio]

enable_audio_input=true

[autoload]

